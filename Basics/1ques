// Online C++ compiler to run C++ program online
#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
  int a;
   cout<<"Enter the amount :";
   cin>>a;
   cout<<endl;
   int ch;
   cout<<"Enter choice : \n 1.Rs.500 \n 2.Rs.100\n 3.Rs.50 \n 4. Rs.20 \n 5.Rs.10"<<endl;
   cin>>ch;
  
       switch(ch){
           case 1 : cout<<" Notes of Rs.500 is :"<<a/500;
           break; 
           case 2 :cout<<" Notes of Rs.100 is :"<<a/100;
           break;
           case 3: cout<<" Notes of Rs.50 is :"<<a/50;
           break;
           case 4 :cout<<" Notes of Rs.20 is :"<<a/20;
           break;
           case 5 :cout<<" Notes of Rs.10 is :"<<a/10;
           break;
           default : cout<<"Enter valid choice!"<<endl;
           break;
       }
//       Enter the amount :2000

// Enter choice : 
//  1.Rs.500 
//  2.Rs.100
//  3.Rs.50 
//  4. Rs.20 
//  5.Rs.10
// 1
//  Notes of Rs.500 is :4	 	 	
   
    return 0;
}
