#include <iostream>
using namespace std;
bool isprime(int n){
    for(int i=2;i<n;i++){
        if(n%i==0){
            return false;
        }
    }
    return true;
}
int main() {
  int a;
  cin >> a;

  if(isprime(a)){
      cout<<"prime no.";
  }
   else cout<<" not a prime no.";
//  45
//  not a prime no.
// 41
// prime no.

    return 0;
}
