// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int power(int a, int b){
    int ans=1;
    for(int i=1;i<=b;i++){
        ans=ans*a;
    }
    return ans;


}
int main() {
  int a;
  int b;
  cin >> a >> b;
  cout<<endl;
  cout<<power(a,b);
   
// 2 4

// 16
    return 0;
}
