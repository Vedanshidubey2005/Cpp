class Solution {
public:
    vector<int> getFloorAndCeil(vector<int> nums, int x) {
           int n = nums.size();
           int low = 0 , high = n-1;
           int ans = -1;
           int ans2= -1;
           vector<int> ans1;
           while(low<=high){
            int mid = (low + high)/2;
            if(nums[mid]<= x){
                ans=nums[mid];
                low=mid+1;
            }
            else{
                high=mid-1;
            }
           }
           ans1.push_back(ans);
            while(low<=high){
            int mid = (low+high)/2;
            if(nums[mid]>=x) {
                ans2= nums[mid];
                high = mid-1;
            }
            else{
                low = mid+1;
            }
        }
         ans1.push_back(ans2);
         return ans1;
    }
};
